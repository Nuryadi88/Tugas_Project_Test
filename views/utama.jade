extends layout

block content
  h1  selamat datang  
    a(href='/login')  Login 
  div
    table(border='1')
      thead
        th CODE
        th NAME
        th DEPARTMENT
        th AGE

      tbody
        each s in students
          tr
            td #{s.code}
            td #{s.name}
            td #{s.department}
            td #{s.age}
  